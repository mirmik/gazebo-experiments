<?xml version="1.0"?>
<sdf version="1.4">
  <world name="default">
    <plugin name="world" filename="libworld.so"/>
    <gravity>0 0 -1</gravity>
  
    <include>
      <uri>model://sun</uri>
    </include>

    <include>
      <uri>model://ground_plane</uri>
    </include>

    
    <model name="spider">
      <pose>0 0 2 0 0 0</pose>

      <link name="body">
        <collision name="collision_body_0">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <box>
            	<size>1 3 0.5</size>
            </box>    
          </geometry>
        </collision>

        <visual name="visual_body_0">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <box>
            	<size>1 3 0.5</size>
            </box>    
          </geometry>
        </visual>
      </link>

      
	  <link name="shoulder_0">
        <collision name="collision_shoulder_0_0">
          <pose>-0.5  1.5  0  0  0  0   </pose>
          <geometry>
            <sphere>
            	<radius>0.3</radius>
            </sphere>    
          </geometry>
        </collision>
  
        <visual name="visual_shoulder_0_0">
          <pose>-0.5  1.5  0  0  0  0   </pose>
          <geometry>
            <sphere>
            	<radius>0.3</radius>
            </sphere>    
          </geometry>
        </visual>
      </link>


	  <link name="high_leg_0">
        <collision name="collision_high_leg_0_0">
          <pose>-1.5  1.5  0  0  0  0   </pose>
          <geometry>
            <box>
            	<size>2 0.5 0.5</size>
            </box>    
          </geometry>
        </collision>
  
        <visual name="visual_high_leg_0_0">
          <pose>-1.5  1.5  0  0  0  0   </pose>
          <geometry>
            <box>
            	<size>2 0.5 0.5</size>
            </box>    
          </geometry>
        </visual>
      </link>

      <link name="low_leg_0">
        <collision name="collision_low_leg_0_0">
          <pose>-3.5  1.5  0  0  0  0   </pose>
          <geometry>
            <box>
            	<size>2 0.5 0.5</size>
            </box>    
          </geometry>
        </collision>
  
        <visual name="visual_low_leg_0_0">
          <pose>-3.5  1.5  0  0  0  0   </pose>
          <geometry>
            <box>
            	<size>2 0.5 0.5</size>
            </box>    
          </geometry>
        </visual>
      </link>	

      <link name="fin_leg_0">
        <collision name="collision_fin_leg_0_0">
          <pose>-5.5  1.5  0  0  0  0   </pose>
          <geometry>
            <sphere>
            	<radius>0.5</radius>
            </sphere>    
          </geometry>
        </collision>
  
        <visual name="visual_fin_leg_0_0">
          <pose>-5.5  1.5  0  0  0  0   </pose>
          <geometry>
            <sphere>
            	<radius>0.5</radius>
            </sphere>    
          </geometry>
        </visual>
      </link>	

      <joint name="joint_shoulder_0" type="revolute">
      	 <pose> -0.5  1.5  0  0  0  0    </pose>
         <parent>body</parent>
         <child>shoulder_0</child>
         <axis>
         	<xyz>0 0 1</xyz>
	        <use_parent_model_frame>true</use_parent_model_frame>
	     </axis>
      </joint>

      <joint name="joint_high_0" type="revolute">
      	 <pose> -0.5  1.5  0  0  0  0    </pose>
         <parent>shoulder_0</parent>
         <child>high_leg_0</child>
         <axis>
         	<xyz>0 1 0</xyz>
	        <use_parent_model_frame>true</use_parent_model_frame>
	     </axis>
      </joint>

      <joint name="joint_low_0" type="revolute">
      	 <pose> -2.5  1.5  0  0  0  0    </pose>
         <parent>high_leg_0</parent>
         <child>low_leg_0</child>
         <axis>
          <xyz>0 1 0</xyz>
	      <use_parent_model_frame>true</use_parent_model_frame>
         </axis>
      </joint>

      <joint name="joint_fin_0" type="fixed">
      	 <pose> -4.5  1.5  0  0  0  0    </pose>
         <parent>low_leg_0</parent>
         <child>fin_leg_0</child>
      </joint>

    </model>


  </world>
</sdf>
